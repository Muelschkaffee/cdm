{
  "jsonSchemaSemanticVersion": "1.1.0",
  "imports": [
    {
      "corpusPath": "cdm:/foundations.4.3.cdm.json"
    },
    {
      "corpusPath": "Sensor.1.0.cdm.json"
    },
    {
      "corpusPath": "Unit.1.0.cdm.json"
    }
  ],
  "definitions": [
    {
      "entityName": "SensorUnit",
      "extendsEntity": "CdmEntity",
      "hasAttributes": [
        {
          "explanation": "fk SensorUnit.SensorId to Sensor.SensorId",
          "entity": {
            "entityReference": "Sensor",
            "appliedTraits": [
              {
                "traitReference": "is.identifiedBy",
                "arguments": [
                  "Sensor/(resolvedAttributes)/SensorId"
                ]
              }
            ]
          },
          "name": "SensorId",
          "resolutionGuidance": {
            "renameFormat": "{m}",
            "entityByReference": {
              "allowReference": true,
              "foreignKeyAttribute": {
                "name": "SensorId",
                "purpose": "hasA",
                "dataType": "string",
                "description": "Unique identifier either coming from previous system otherwise it needs to be defined",
                "maximumLength": 50
              }
            }
          },
          "purpose": "identifiedBy"
        },
        {
          "explanation": "fk SensorUnit.UnitId to Unit.UnitId",
          "entity": {
            "entityReference": "Unit",
            "appliedTraits": [
              {
                "traitReference": "is.identifiedBy",
                "arguments": [
                  "Unit/(resolvedAttributes)/UnitId"
                ]
              }
            ]
          },
          "name": "UnitId",
          "resolutionGuidance": {
            "renameFormat": "{m}",
            "entityByReference": {
              "allowReference": true,
              "foreignKeyAttribute": {
                "name": "UnitId",
                "purpose": "hasA",
                "dataType": "string",
                "description": "Unique identifier either coming from previous system otherwise it needs to be defined",
                "maximumLength": 50
              }
            }
          },
          "purpose": "identifiedBy"
        }
      ],
      "displayName": "SensorUnit",
      "description": "Linking entity between Sensor and Unit. The sensor represents the sensor measurement area which can be different from the physical location "
    }
  ]
}