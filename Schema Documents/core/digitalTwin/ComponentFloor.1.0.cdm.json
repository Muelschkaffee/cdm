      "hasAttributes": [
        {
          "explanation": "fk ComponentFloor.ComponentId to Component.ComponentId",
          "entity": {
            "entityReference": "Component",
            "appliedTraits": [
              {
                "traitReference": "is.identifiedBy",
                "arguments": [
                  "Component/(resolvedAttributes)/ComponentId"
                ]
              }
            ]
          },
          "name": "ComponentId",
          "resolutionGuidance": {
            "renameFormat": "{m}",
            "entityByReference": {
              "allowReference": true,
              "foreignKeyAttribute": {
                "name": "ComponentId",
                "purpose": "hasA",
                "dataType": "string",
                "description": "Unique identifier either coming from previous system otherwise it needs to be defined",
                "maximumLength": 50
              }
            }
          },
          "purpose": "identifiedBy"
        },
        {
          "explanation": "fk ComponentFloor.FloorId to Floor.FloorId",
          "entity": {
            "entityReference": "Floor",
            "appliedTraits": [
              {
                "traitReference": "is.identifiedBy",
                "arguments": [
                  "Floor/(resolvedAttributes)/FloorId"
                ]
              }
            ]
          },
          "name": "FloorId",
          "resolutionGuidance": {
            "renameFormat": "{m}",
            "entityByReference": {
              "allowReference": true,
              "foreignKeyAttribute": {
                "name": "FloorId",
                "purpose": "hasA",
                "dataType": "string",
                "description": "Unique identifier either coming from previous system otherwise it needs to be defined",
                "maximumLength": 50
              }
            }
          },
          "purpose": "identifiedBy"
        }
      ],
      "displayName": "ComponentFloor",
      "description": "Linking entity between component and floor"
    }
  ]
}