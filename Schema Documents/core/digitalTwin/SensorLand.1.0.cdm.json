{
  "jsonSchemaSemanticVersion": "1.1.0",
  "imports": [
    {
      "corpusPath": "cdm:/foundations.4.3.cdm.json"
    },
    {
      "corpusPath": "Sensor.1.0.cdm.json"
    },
    {
      "corpusPath": "Land.1.0.cdm.json"
    }
  ],
  "definitions": [
    {
      "entityName": "SensorLand",
      "extendsEntity": "CdmEntity",
      "hasAttributes": [
        {
          "explanation": "fk SensorLand.SensorId to Sensor.SensorId",
          "entity": {
            "entityReference": "Sensor",
            "appliedTraits": [
              {
                "traitReference": "is.identifiedBy",
                "arguments": [
                  "Sensor/(resolvedAttributes)/SensorId"
                ]
              }
            ]
          },
          "name": "SensorId",
          "resolutionGuidance": {
            "renameFormat": "{m}",
            "entityByReference": {
              "allowReference": true,
              "foreignKeyAttribute": {
                "name": "SensorId",
                "purpose": "hasA",
                "dataType": "string",
                "description": "Unique identifier either coming from previous system otherwise it needs to be defined",
                "maximumLength": 50
              }
            }
          },
          "purpose": "identifiedBy"
        },
        {
          "explanation": "fk SensorLand.LandId to Land.LandId",
          "entity": {
            "entityReference": "Land",
            "appliedTraits": [
              {
                "traitReference": "is.identifiedBy",
                "arguments": [
                  "Land/(resolvedAttributes)/LandId"
                ]
              }
            ]
          },
          "name": "LandId",
          "resolutionGuidance": {
            "renameFormat": "{m}",
            "entityByReference": {
              "allowReference": true,
              "foreignKeyAttribute": {
                "name": "LandId",
                "purpose": "hasA",
                "dataType": "string",
                "description": "Unique identifier either coming from previous system otherwise it needs to be defined",
                "maximumLength": 50
              }
            }
          },
          "purpose": "identifiedBy"
        }
      ],
      "displayName": "SensorLand",
      "description": "Linking entity between Sensor and Land. The sensor represents the sensor measurement area which can be different from the physical location "
    }
  ]
}