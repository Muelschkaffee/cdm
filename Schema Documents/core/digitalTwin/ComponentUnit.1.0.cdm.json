{
  "jsonSchemaSemanticVersion": "1.1.0",
  "imports": [
    {
      "corpusPath": "cdm:/foundations.4.3.cdm.json"
    },
    {
      "corpusPath": "Component.1.0.cdm.json"
    },
    {
      "corpusPath": "Unit.1.0.cdm.json"
    }
  ],
  "definitions": [
    {
      "entityName": "ComponentUnit",
      "extendsEntity": "CdmEntity",
      "hasAttributes": [
        {
          "explanation": "fk ComponentUnit.ComponentId to Component.ComponentId",
          "entity": {
            "entityReference": "Component",
            "appliedTraits": [
              {
                "traitReference": "is.identifiedBy",
                "arguments": [
                  "Component/(resolvedAttributes)/ComponentId"
                ]
              }
            ]
          },
          "name": "ComponentId",
          "resolutionGuidance": {
            "renameFormat": "{m}",
            "entityByReference": {
              "allowReference": true,
              "foreignKeyAttribute": {
                "name": "ComponentId",
                "purpose": "hasA",
                "dataType": "string",
                "description": "Unique identifier either coming from previous system otherwise it needs to be defined",
                "maximumLength": 50
              }
            }
          },
          "purpose": "identifiedBy"
        },
        {
          "explanation": "fk ComponentUnit.UnitId to Unit.UnitId",
          "entity": {
            "entityReference": "Unit",
            "appliedTraits": [
              {
                "traitReference": "is.identifiedBy",
                "arguments": [
                  "Unit/(resolvedAttributes)/UnitId"
                ]
              }
            ]
          },
          "name": "UnitId",
          "resolutionGuidance": {
            "renameFormat": "{m}",
            "entityByReference": {
              "allowReference": true,
              "foreignKeyAttribute": {
                "name": "UnitId",
                "purpose": "hasA",
                "dataType": "string",
                "description": "Unique identifier either coming from previous system otherwise it needs to be defined",
                "maximumLength": 50
              }
            }
          },
          "purpose": "identifiedBy"
        }
      ],
      "displayName": "ComponentUnit",
      "description": "Linking entity between component and unit"
    }
  ]
}