          "entity": {
            "entityReference": "Certificate",
            "appliedTraits": [
              {
                "traitReference": "is.identifiedBy",
                "arguments": [
                  "Certificate/(resolvedAttributes)/CertificateId"
                ]
              }
            ]
          },
          "name": "CertificateId",
          "resolutionGuidance": {
            "renameFormat": "{m}",
            "entityByReference": {
              "allowReference": true,
              "foreignKeyAttribute": {
                "name": "CertificateId",
                "purpose": "hasA",
                "dataType": "string",
                "description": "Unique identifier either coming from previous system otherwise it needs to be defined",
                "maximumLength": 50
              }
            }
          },
          "purpose": "identifiedBy"
        },
        {
          "explanation": "fk CertificateBuilding.BuildingId to Building.BuildingId",
          "entity": {
            "entityReference": "Building",
            "appliedTraits": [
              {
                "traitReference": "is.identifiedBy",
                "arguments": [
                  "Building/(resolvedAttributes)/BuildingId"
                ]
              }
            ]
          },
          "name": "BuildingId",
          "resolutionGuidance": {
            "renameFormat": "{m}",
            "entityByReference": {
              "allowReference": true,
              "foreignKeyAttribute": {
                "name": "BuildingId",
                "purpose": "hasA",
                "dataType": "string",
                "description": "Unique identifier either coming from previous system otherwise it needs to be defined",
                "maximumLength": 50
              }
            }
          },
          "purpose": "identifiedBy"
        }
      ],
      "displayName": "CertificateBuilding",
      "description": "Linking entity between certificate and building"
    }
  ]
